{
    "compilerOptions": {
        "moduleResolution": "node",
        "module": "es2015",
        "target": "es2015",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "strictNullChecks": true,
        "noImplicitAny": true,
        "listFiles": true,
        "sourceMap": true,
        "declaration": false,
        "pretty": true,
        "outDir": "built"
    },
    "lib": [
        "es5", "es2015", "es2016"
    ],
    "files": [
        "src/index"
    ]
}
